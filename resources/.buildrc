{
	"less": {
		"ignore": false,
		"input": [
			"less/seo.less",
			"less/redirects.less",
			"less/settings.less"
		],
		"output": [
			"../src/web/assets/css/seo.css",
			"../src/web/assets/css/redirects.css",
			"../src/web/assets/css/settings.css"
		],
		"watch": [
			"less/**/*"
		]
	},
	"js": {
		"ignore": false,
		"input": [
			"js/SeoField.js",
			"js/SeoSettings.js",
			"js/SeoFieldSettings.js"
		],
		"output": [
			"../src/web/assets/js/SeoField.min.js",
			"../src/web/assets/js/SeoSettings.min.js",
			"../src/web/assets/js/SeoFieldSettings.min.js"
		],
		"watch": [
			"js/**/*.js",
			"!js/**/*.min.js"
		]
	},
	"browserSync": {
		"ignore": true
	}
}