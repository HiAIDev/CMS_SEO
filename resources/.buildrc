{
	"less": {
		"ignore": false,
		"input": "less/**/*.less",
		"output": "../seo/resources/css",
		"watch": [
			"less/**/*"
		]
	},
	"js": {
		"ignore": false,
		"input": [
			"js/SeoField.js",
			"js/SeoSettings.js",
			"js/SeoSchema.js"
		],
		"output": [
			"../seo/resources/js/SeoField.min.js",
			"../seo/resources/js/SeoSettings.min.js",
			"../seo/resources/js/SeoSchema.min.js"
		],
		"watch": [
			"js/**/*.js",
			"!js/**/*.min.js"
		]
	},
	"browserSync": {
		"ignore": true
	}
}